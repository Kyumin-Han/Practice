import java.util.*;
public class Programmers5 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		
		int num;
		System.out.print("숫자를 입력하세요 : ");
		num = input.nextInt();
		
		input.close();
		
		solution(num);

	}
	
	public static int solution(int n) {
		int answer = 0;
		
		for(int i = 2; i <= n; i++)
		{
			for(int j = 2; j < i; j++)
			{
				
			}
		}
		
		return answer;
	}

}
