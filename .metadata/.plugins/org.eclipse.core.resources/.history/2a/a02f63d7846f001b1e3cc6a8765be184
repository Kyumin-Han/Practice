package chap01;

import java.util.Scanner;

public class Max3 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		int a, b, c;
		
		a = input.nextInt();
		b = input.nextInt();
		c = input.nextInt();
		
		int max = a;
		
		if(b > max)
		{
			max = b;
		}
		
		if(c > max) 
		{
			max = c;
		}

	}

}
