package Lambda;

public class Main2 {

	public static void main(String[] args) {
		Flyable flyable = new FlyingCar();
		flyable.fly();
		
		Flyable flyable2 = new Flyable() {

			@Override
			public void fly() {
				System.out.println("하늘을 납니다.");
			}
		};
		
		flyable2.fly();
		
		Flyable flyable3 = () -> System.out.println("종이비행기가 날아갑니다.");

	}

}
