package Lambda;

import java.util.*;

public class Main {

	public static void main(String[] args) {
		
//		ArrayList <Integer> list = new ArrayList <>();
		
		
		List<Integer> list = new ArrayList<>();
		for(int i = 1; i <= 7; i++) list.add(i);
		
		for(Integer i : list)
		{
			System.out.println(i);
		}
	}

}
